{
  "expo": {
    "name": "treasurehuntapp",
    "slug": "treasurehuntapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.anonymous.treasurehuntapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEF9F6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.treasurehuntapp",
      "infoPlist": {
        "infoPlist": {
          "UIBackgroundModes": ["location", "fetch", "remote-notification"],
          "NSLocationWhenInUseUsageDescription": "This app requires access to your location when open.",
          "NSLocationAlwaysAndWhenInUseUsageDescription": "This app requires access to your location even when closed.",
          "NSLocationAlwaysUsageDescription": "This app requires access to your location when open.",
          "CFBundleURLTypes": [
            {
              "CFBundleURLSchemes": [
                "com.googleusercontent.apps..499556521140-u23ghdeve49lepfqhjthtast2gl2epha"
              ]
            }
          ]
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.treasurehuntapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Oswald-Medium.ttf"]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "484ef1a1-c9e8-4a04-b817-824398c77cf9"
      }
    },
    "owner": "jonathanatger"
  }
}
